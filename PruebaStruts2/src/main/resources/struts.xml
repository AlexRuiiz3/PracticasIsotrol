<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

  <package name="default" extends="struts-default">
    <action name="tareaEntregaVerCoches" method="verCoches" class="com.isotrol.action.CocheAction">
      <result name="success">/indexCoches.jsp</result>
      <result name="error">/viewError.jsp</result>
    </action>
    
     <action name="tareaEntregaGuardarCoche" method="guardarCoche" class="com.isotrol.action.CocheAction">
      <result name="success">/guardarCoches.jsp</result>
      <result name="error">/viewError.jsp</result>
    </action>
   </package>
   
</struts>
    